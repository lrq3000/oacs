    "cansgaussian_mergeclusters": false,
    "optimization": {
        "post": {
            "crossval": {
                "variables": {
                    "Theta": [0.1, 0.2, 0.3],
                    "Lambda": "xrange(1,10)",
                    "K": "xrange(1,6)",
                    "T": {
                        "min": 0,
                        "max": 10,
                        "func": "lambda x: -log(x)",
                        "type": "pas de type! float pas de int - pour dichotomy"
                        }
                    },
                "costfunc": "01Cost"
                }
            }
        }
